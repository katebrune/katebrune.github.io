{
  "name": "katebrune.github.io",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "test:components": "jest --testPathIgnorePatterns=ui-tests/",
    "test:visual-regression": "jest ui-tests/visual-regression.test.ts",
    "test:visual-regression-ci": "yarn build-storybook-docker && start-server-and-test storybook-docker http://localhost:6006 test:visual-regression",
    "test:a11y": "jest ui-tests/a11y.test.ts",
    "test:a11y-ci": "yarn build-storybook-docker && start-server-and-test storybook-docker http://localhost:6006 test:a11y",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build-storybook-docker": "docker build -f .storybook/Dockerfile -t storybook-docker . --progress=plain",
    "storybook-docker": "docker run --rm -p 6006:8080 storybook-docker"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@radix-ui/react-separator": "^1.0.2",
    "@storybook/addon-viewport": "^7.0.2",
    "classnames": "^2.3.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "gray-matter": "^4.0.3",
    "moment": "^2.29.4",
    "next": "^13.2.4",
    "next-mdx-remote": "^4.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@storybook/addon-a11y": "^7.0.2",
    "@storybook/addon-actions": "^7.0.2",
    "@storybook/addon-essentials": "^7.0.2",
    "@storybook/addon-links": "^7.0.2",
    "@storybook/addon-mdx-gfm": "^7.0.2",
    "@storybook/addon-storyshots": "^7.0.2",
    "@storybook/addon-storyshots-puppeteer": "^7.0.2",
    "@storybook/nextjs": "^7.0.2",
    "@storybook/react": "^7.0.2",
    "@tailwindcss/line-clamp": "^0.2.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@types/react": "17.0.32",
    "autoprefixer": "^10.3.7",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^7.32.0",
    "eslint-config-next": "^13.2.4",
    "eslint-plugin-storybook": "^0.6.11",
    "husky": ">=6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "lint-staged": ">=10",
    "postcss": "^8.3.11",
    "prettier": "^2.4.1",
    "puppeteer": "^11.0.0",
    "start-server-and-test": "^2.0.0",
    "storybook": "^7.0.2",
    "storybook-css-modules": "^1.0.8",
    "tailwindcss": "^3.3.1",
    "typescript": "4.4.4"
  },
  "resolutions": {
    "@storybook/nextjs/resolve-url-loader/adjust-sourcemap-loader/loader-utils": "^2.0.4",
    "minimatch": "^3.0.5",
    "minimist": "^1.2.6",
    "json5": "^2.2.2",
    "qs": "^6.10.3",
    "nanoid": "^3.1.31"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
