{
  "name": "katebrune.github.io",
  "version": "0.1.4",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "test:components": "jest --testPathIgnorePatterns=ui-tests/",
    "test:visual-regression": "jest ui-tests/visual-regression.test.ts",
    "test:visual-regression-ci": "yarn build-storybook-docker && start-server-and-test storybook-docker http://localhost:6006 test:visual-regression",
    "test:a11y": "jest ui-tests/a11y.test.ts",
    "test:a11y-ci": "yarn build-storybook-docker && start-server-and-test storybook-docker http://localhost:6006 test:a11y",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build-storybook-docker": "docker build -f .storybook/Dockerfile -t storybook-docker . --progress=plain",
    "storybook-docker": "docker run --rm -p 6006:8080 storybook-docker"
  },
  "dependencies": {
    "@mdx-js/loader": "^1.6.22",
    "@storybook/addon-viewport": "^6.3.12",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "gray-matter": "^4.0.3",
    "moment": "^2.29.1",
    "next": "^13.2.4",
    "next-mdx-remote": "^3.0.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.1.13",
    "remark-prism": "^1.3.6",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@storybook/addon-a11y": "^6.4.4",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-storyshots": "^6.3.12",
    "@storybook/addon-storyshots-puppeteer": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "@tailwindcss/line-clamp": "^0.2.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@types/nprogress": "^0.2.0",
    "@types/react": "17.0.32",
    "autoprefixer": "^10.3.7",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^7.32.0",
    "eslint-config-next": "^13.2.4",
    "husky": ">=6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "lint-staged": ">=10",
    "postcss": "^8.3.11",
    "prettier": "^2.4.1",
    "puppeteer": "^11.0.0",
    "react-test-renderer": "^17.0.2",
    "start-server-and-test": "^1.14.0",
    "storybook-css-modules": "^1.0.8",
    "storybook-dark-mode": "^2.0.5",
    "tailwindcss": "^2.2.17",
    "typescript": "4.4.4"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
