{
  "name": "katebrune.github.io",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "test:components": "jest",
